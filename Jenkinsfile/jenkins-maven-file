pipeline {
    agent none
    parameters {
        choice(
            choices: ['Yes','NO'],
            description: 'Choose if the CDT file need to be Deployed',
            name: 'CDPFILE'
        )
    }
    stages {
        stage("To upload a file") {
            when {
                expression {params.CDPFILE == "YES"}
            }
            agent {label 'partycity-ecs-dev'}
            steps {
                checkout scm
                echo "The file hs been uploded"

            }
        }
    }
}
